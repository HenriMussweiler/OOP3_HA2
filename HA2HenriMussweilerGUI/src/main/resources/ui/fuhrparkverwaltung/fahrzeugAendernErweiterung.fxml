<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ui.fuhrparkverwaltung.FahrzeugAendernErweiterungController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Label layoutX="70.0" layoutY="44.0" text="Fahrzeug bearbeiten">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="herstellerField" layoutX="281.0" layoutY="99.0" promptText="Hersteller" />
        <TextField fx:id="modellField" layoutX="281.0" layoutY="149.0" promptText="Modell" />
        <TextField fx:id="ausstattungField" layoutX="281.0" layoutY="199.0" promptText="Ausstattungsvariante" />
        <TextField fx:id="leistungField" layoutX="281.0" layoutY="249.0" promptText="Leistung in KW" />
        <TextField fx:id="kilometerstandField" layoutX="281.0" layoutY="299.0" promptText="Kilometerstand" />
        <ComboBox fx:id="kraftstoffField" layoutX="281.0" layoutY="349.0" promptText="Kraftstoffart" />
        <TextField fx:id="baujahrField" layoutX="281.0" layoutY="397.0" promptText="Baujahr" />
        <ComboBox fx:id="getriebeField" layoutX="281.0" layoutY="449.0" promptText="Art des Getriebes" />
        <TextField fx:id="sitzplaetzeField" layoutX="281.0" layoutY="499.0" promptText="Sitzplätze" />
        <Button fx:id="speichernButton" layoutX="368.0" layoutY="598.0" onAction="#speichernButtonClicked" text="Speichern" />
        <Button fx:id="zurueckButton" layoutX="86.0" layoutY="598.0" mnemonicParsing="false" onAction="#zurueckButtonClicked" text="Zurück" />
        <ComboBox fx:id="sharingComboBox" layoutX="281.0" layoutY="548.0" prefHeight="26.0" prefWidth="160.0" promptText="Standort" />
        <Label layoutX="86.0" layoutY="104.0" text="Hersteller:" />
        <Label layoutX="86.0" layoutY="154.0" text="Modell:" />
        <Label layoutX="86.0" layoutY="204.0" text="Ausstattungsvariante:" />
        <Label layoutX="86.0" layoutY="254.0" text="Leistung in KW:" />
        <Label layoutX="86.0" layoutY="304.0" text="Kilometerstand:" />
        <Label layoutX="86.0" layoutY="402.0" text="Baujahr:" />
        <Label layoutX="86.0" layoutY="504.0" text="Sitzplätze:" />
        <Label layoutX="86.0" layoutY="553.0" text="Standort:" />
        <Label layoutX="85.0" layoutY="453.0" text="Getriebeart:" />
        <Label layoutX="85.0" layoutY="353.0" text="Kraftstoffart." />
    </children>
</AnchorPane>
