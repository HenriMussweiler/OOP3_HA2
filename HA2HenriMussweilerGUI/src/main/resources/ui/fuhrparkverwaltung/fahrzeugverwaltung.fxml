<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.fuhrparkverwaltung.FahrzeugverwaltungController">
   <children>
      <Label alignment="TOP_CENTER" layoutX="200.0" layoutY="70.0" prefWidth="210.0" text="Fuhrparkverwaltung:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="fahrzeugAnlegenButton" layoutX="210.0" layoutY="150.0" mnemonicParsing="false" onAction="#fahrzeugAnlegenButtonClicked" prefWidth="180.0" text="Fahrzeug anlegen" />
      <Button fx:id="fahrzeugAendernButton" layoutX="210.0" layoutY="190.0" mnemonicParsing="false" onAction="#fahrzeugAendernButtonClicked" prefWidth="180.0" text="Fahrzeug ändern" />
      <Button fx:id="fahrzeugLoeschenButton" layoutX="210.0" layoutY="230.0" mnemonicParsing="false" onAction="#fahrzeugLoeschenButtonClicked" prefWidth="180.0" text="Fahrzeug löschen" />
      <Button fx:id="standortButton" layoutX="210.0" layoutY="110.0" mnemonicParsing="false" onAction="#standortButtonClicked" prefWidth="180.0" text="Sharing Standort hinzufügen" />
      <Button fx:id="zurueckButton" layoutX="210.0" layoutY="270.0" mnemonicParsing="false" onAction="#zurueckButtonClicked" prefWidth="180.0" text="Zurück" />
   </children>
</AnchorPane>
